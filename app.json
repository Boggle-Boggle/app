{
  "expo": {
    "name": "빼곡 - 빼곡하게 채우는 나만의 책장",
    "slug": "app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "bbaegok",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#EEEDEB"
    },
    "ios": {
      "icon": "./assets/images/logo.png",
      "supportsTablet": false,
      "bundleIdentifier": "com.bbaegok.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logofore.png",
        "backgroundColor": "#EEEDEB"
      },
      "package": "bbaegok.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "66be81d8-1ef2-4252-9be3-d9e376592645"
      }
    }
  }
}
